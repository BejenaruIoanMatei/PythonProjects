   "source": [
    "import math as m\n",
    "\n",
    "def Entropy(X):\n",
    "    return -sum(i * m.log2(i) for i in X if i > 0)\n",
    "\n",
    "X = [0.15, 0.35, 0.5]\n",
    "print(f\"H(X) = {Entropy(X)}\")"
   ]
